local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager, InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))(), loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
local HttpService, Players, Workspace, ReplicatedStorage, VirtualUser = game:GetService("HttpService"), game:GetService("Players"), game:GetService("Workspace"), game:GetService("ReplicatedStorage"), game:GetService("VirtualUser")
local Window = Fluent:CreateWindow({Title = "Liz HUB", SubTitle = "", TabWidth = 160, Size = UDim2.fromOffset(580, 520), Acrylic = true, Theme = "Dark", MinimizeKey = Enum.KeyCode.RightControl})
local LocalPlayer, Remote = Players.LocalPlayer, ReplicatedStorage:WaitForChild("Modules"):WaitForChild("Signal"):WaitForChild("RemoteEvent")
local DungeonRemote, ItemRemote = ReplicatedStorage.NEW37.DUNGEONS.DungeonEvent, ReplicatedStorage.RemoteEvents.ItemRemote

local Config = {AutoDungeon=false, AutoTower=false, SelectedDungeonRank="B", AutoFarm=false, AuraKill=false, AutoMadara=false, AutoItachi=false, AutoUseDB=false, SkillDelay=0, OffVFX=false, ReduceLag=false, AntiAFK=false, HoverHeight=12, SelectedTP="Broly"}
local DungeonData = {["B"]={Name="B Dungeon Ticket",Var="BDTickets"}, ["A"]={Name="A Dungeon Ticket",Var="ADTickets"}, ["S"]={Name="S Dungeon Ticket",Var="SDTickets"}, ["SS"]={Name="SS Dungeon Ticket",Var="SSDTickets"}}
local TeleportData = {["Broly"]={TargetName="Legendary Saiyan6",Coords=CFrame.new(1402,-3,143)}, ["Tanjiro"]={TargetName="Tanjiro",Coords=CFrame.new(1510,17,-104)}, ["Prestige NPC"]={TargetName="PrestigeNPC",Coords=CFrame.new(611,222,833)}, ["Sukuna"]={TargetName="Sukuna",Coords=CFrame.new(870,122,526)}, ["Zoro"]={TargetName="Zoro (PTS)",Coords=CFrame.new(557,5,-182)}, ["Cell"]={TargetName="Cell",Coords=CFrame.new(1094,21,-912)}, ["Shop"]={TargetName="Shop",Coords=CFrame.new(-829,9,-1087)}}
local Tabs = {Main=Window:AddTab({Title="Auto Dungeon",Icon="castle"}), Farm=Window:AddTab({Title="Auto Farm",Icon="sword"}), Teleport=Window:AddTab({Title="Teleport",Icon="map"}), Misc=Window:AddTab({Title="Misc",Icon="component"}), Settings=Window:AddTab({Title="Settings",Icon="settings"})}

function ToggleAntiGravity(s) local c=LocalPlayer.Character; if not c or not c:FindFirstChild("HumanoidRootPart") then return end; if s then if not c.HumanoidRootPart:FindFirstChild("JemAntiGravity") then local b=Instance.new("BodyVelocity");b.Name="JemAntiGravity";b.Velocity=Vector3.zero;b.MaxForce=Vector3.new(1e5,1e5,1e5);b.Parent=c.HumanoidRootPart end else local b=c.HumanoidRootPart:FindFirstChild("JemAntiGravity");if b then b:Destroy() end end end
local function LookAt(t) if t then Workspace.CurrentCamera.CFrame=CFrame.new(Workspace.CurrentCamera.CFrame.Position,t.Position) end end
local function GetPartyID() local r=LocalPlayer:FindFirstChild("ReiNew"); if r then for _,v in pairs(r:GetChildren()) do if v:IsA("StringValue") and (v.Name:lower():match("party") or v.Name:lower():match("id")) and #v.Value>10 then return v.Value end end end; local p=LocalPlayer:FindFirstChild("PlayerValues"); if p then local v=p:FindFirstChild("PartyID") or p:FindFirstChild("SquadID"); if v then return v.Value end end; return nil end

task.spawn(function() while true do -- VFX
    if Config.ReduceLag then pcall(function() for _,g in pairs(LocalPlayer.PlayerGui:GetChildren()) do if g.Name:lower():match("damage") or g.Name:lower():match("indicator") then g.Enabled=false end end end) else pcall(function() for _,g in pairs(LocalPlayer.PlayerGui:GetChildren()) do if g.Name:lower():match("damage") then g.Enabled=true end end end) end
    if Config.OffVFX then pcall(function() LocalPlayer.Backpack.Scripts.NewModules:Destroy() end); for _,o in pairs(Workspace:GetChildren()) do if o.Name:match("HitPart") or o.Name:match("Splash") then o:Destroy() end end; for _,n in pairs({"Particles","NewEffects","KevSFX","VFX","ReiNew"}) do if ReplicatedStorage:FindFirstChild(n) then ReplicatedStorage[n]:Destroy() end end end
    task.wait(1)
end end)

task.spawn(function() while true do -- Combat
    local c=LocalPlayer.Character; if Config.AutoMadara then ReplicatedStorage.Remotes.AbilityHandler:FireServer("MadaraFifth") end
    if Config.AutoItachi and c and c:FindFirstChild("HumanoidRootPart") then ReplicatedStorage.RemoteEvents.ItachiSusanooRibcageRemote:FireServer(false,c.HumanoidRootPart.Position,c.HumanoidRootPart.CFrame,1) end
    if Config.AuraKill then ReplicatedStorage.RemoteEvents.BladeCombatRemote:FireServer() end
    task.wait(Config.SkillDelay>0 and Config.SkillDelay or 0.1)
end end)

task.spawn(function() while true do -- Auto Use DB Only
    if Config.AutoUseDB then
        local bp = LocalPlayer:FindFirstChild("Backpack")
        if bp then for _,v in pairs(bp:GetChildren()) do if v.Name:lower():match("dragonball") then pcall(function() ItemRemote:FireServer(v.Name) end) end end end
    end
    task.wait(1)
end end)

task.spawn(function() while true do -- Loop Mode
    local c = LocalPlayer.Character
    if Config.AutoDungeon then -- Dungeon
        if not Workspace:FindFirstChild("Floors") then
            local s=DungeonData[Config.SelectedDungeonRank]; if s and LocalPlayer.ReiNew[s.Var].Value>0 then DungeonRemote:FireServer("Use Dungeon Ticket",{s.Name,s.Var}); task.wait(1.5); DungeonRemote:FireServer("Start Dungeon"); task.wait(5) end
        else
            local n=Workspace.NPCS:FindFirstChild("DeathTapz"); if n and c then c.HumanoidRootPart.CFrame=n.HumanoidRootPart.CFrame*CFrame.new(0,0,4); LookAt(n.HumanoidRootPart); task.wait(0.5); for _,v in pairs(n:GetDescendants()) do if v:IsA("ProximityPrompt") then fireproximityprompt(v) break end end end
            local t=0; repeat task.wait(1); t=t+1 until t>2 or Workspace.Floors:FindFirstChild("0"); Remote:FireServer("Towers:OpenGate"); task.wait(1)
        end
    elseif Config.AutoTower then -- Tower
        ToggleAntiGravity(true)
        if c and c:FindFirstChild("HumanoidRootPart") then
            if not Workspace:FindFirstChild("Floors") then -- Lobby
                local n=Workspace.NPCS:FindFirstChild("DeathTapz"); if n then c.HumanoidRootPart.CFrame=n.HumanoidRootPart.CFrame*CFrame.new(0,0,3); LookAt(n.HumanoidRootPart); task.wait(0.5); for _,v in pairs(n:GetDescendants()) do if v:IsA("ProximityPrompt") then fireproximityprompt(v) break end end; task.wait(1); Remote:FireServer("Towers:StartTower",GetPartyID()); task.wait(8) end
            else -- In Game
                local f=Workspace:FindFirstChild("Live"); local tm=nil; if f then for _,m in pairs(f:GetChildren()) do if m:FindFirstChild("Humanoid") and m.Humanoid.Health>0 then tm=m; break end end end
                if tm then c.HumanoidRootPart.CFrame=CFrame.new(tm.HumanoidRootPart.Position+Vector3.new(0,12,0),tm.HumanoidRootPart.Position)
                else task.wait(0.5); for _,p in pairs(Workspace:GetDescendants()) do if p:IsA("ProximityPrompt") and p.Parent and (c.HumanoidRootPart.Position-p.Parent.Position).Magnitude<500 then c.HumanoidRootPart.CFrame=p.Parent.CFrame; task.wait(0.2); if fireproximityprompt then fireproximityprompt(p) else p:InputHoldBegin(); task.wait(p.HoldDuration+0.1); p:InputHoldEnd() end; task.wait(0.5) end end end
            end
        end
    elseif Config.AutoFarm then -- Farm
        ToggleAntiGravity(true); if c and c:FindFirstChild("HumanoidRootPart") then local f=Workspace:FindFirstChild("Live"); local tm,md=nil,9999; if f then for _,m in pairs(f:GetChildren()) do if m:FindFirstChild("Humanoid") and m.Humanoid.Health>0 then local d=(m.HumanoidRootPart.Position-c.HumanoidRootPart.Position).Magnitude; if d<md then md=d; tm=m end end end end; if tm then c.HumanoidRootPart.CFrame=CFrame.new(tm.HumanoidRootPart.Position+Vector3.new(0,Config.HoverHeight,0),tm.HumanoidRootPart.Position) end end
    else ToggleAntiGravity(false) end
    task.wait()
end end)

-- UI
Tabs.Main:AddDropdown("Rank",{Title="Rank",Values={"B","A","S","SS"},Default=1,Callback=function(v)Config.SelectedDungeonRank=v end})
Tabs.Main:AddToggle("AD",{Title="Auto Dungeon",Default=false,Callback=function(v)Config.AutoDungeon=v end})
Tabs.Main:AddToggle("AT",{Title="Auto Inf Tower",Default=false,Callback=function(v)Config.AutoTower=v end})
Tabs.Main:AddToggle("ADB",{Title="Auto Use DB",Default=false,Callback=function(v)Config.AutoUseDB=v end})
Tabs.Farm:AddToggle("AK",{Title="Aura Kill",Default=false,Callback=function(v)Config.AuraKill=v end})
Tabs.Farm:AddToggle("AM",{Title="Auto Madara",Default=false,Callback=function(v)Config.AutoMadara=v end})
Tabs.Farm:AddToggle("AI",{Title="Auto Itachi",Default=false,Callback=function(v)Config.AutoItachi=v end})
Tabs.Farm:AddSlider("Delay",{Title="Spam Delay",Default=0,Min=0,Max=1,Rounding=2,Callback=function(v)Config.SkillDelay=v end})
Tabs.Farm:AddToggle("AF",{Title="Auto Farm All",Default=false,Callback=function(v)Config.AutoFarm=v end})
Tabs.Teleport:AddDropdown("Dest",{Title="Destination",Values={"Broly","Tanjiro","Prestige NPC","Sukuna","Zoro","Cell","Shop"},Default=1,Callback=function(v)Config.SelectedTP=v end})
Tabs.Teleport:AddButton({Title="Teleport",Callback=function() local d=TeleportData[Config.SelectedTP]; local t=Workspace.ChatParts:FindFirstChild(d.TargetName) or Workspace.Live:FindFirstChild(d.TargetName); if t then LocalPlayer.Character.HumanoidRootPart.CFrame=(t:IsA("Model") and t.HumanoidRootPart.CFrame or t.CFrame) else LocalPlayer.Character.HumanoidRootPart.CFrame=d.Coords end end})
Tabs.Misc:AddToggle("AntiAFK",{Title="Anti AFK",Default=false,Callback=function(v)Config.AntiAFK=v; if v then LocalPlayer.Idled:Connect(function() if Config.AntiAFK then VirtualUser:CaptureController(); VirtualUser:ClickButton2(Vector2.new()) end end) end end})
Tabs.Misc:AddToggle("VFX",{Title="Off VFX",Default=false,Callback=function(v)Config.OffVFX=v end})
Tabs.Misc:AddToggle("Lag",{Title="Reduce Lag",Default=false,Callback=function(v)Config.ReduceLag=v end})

SaveManager:SetLibrary(Fluent); InterfaceManager:SetLibrary(Fluent); SaveManager:IgnoreThemeSettings(); SaveManager:SetIgnoreIndexes({}); SaveManager:BuildConfigSection(Tabs.Settings); InterfaceManager:BuildInterfaceSection(Tabs.Settings); Window:SelectTab(1); SaveManager:LoadAutoloadConfig()
Fluent:Notify({Title="Liz HUB",Content="Compact 2.7.0 (No Deposit)",Duration=3})
